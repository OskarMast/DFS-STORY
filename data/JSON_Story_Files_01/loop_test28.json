{
  "inkVersion": 21,
  "root": [
    [
      "^Once upon a time...",
      "\n",
      [
        "ev",
        { "^->": "0.2.$r1" },
        { "temp=": "$r" },
        "str",
        { "->": ".^.s" },
        [{ "#n": "$r1" }],
        "/str",
        "/ev",
        { "*": "0.c-0", "flg": 18 },
        {
          "s": [
            "^This Choice goes to the loop.",
            { "->": "$r", "var": true },
            null
          ]
        }
      ],
      [
        "ev",
        { "^->": "0.3.$r1" },
        { "temp=": "$r" },
        "str",
        { "->": ".^.s" },
        [{ "#n": "$r1" }],
        "/str",
        "/ev",
        { "*": "0.c-1", "flg": 18 },
        { "s": ["^Another Option", { "->": "$r", "var": true }, null] }
      ],
      [
        "ev",
        { "^->": "0.4.$r1" },
        { "temp=": "$r" },
        "str",
        { "->": ".^.s" },
        [{ "#n": "$r1" }],
        "/str",
        "/ev",
        { "*": "0.c-2", "flg": 18 },
        {
          "s": [
            "^There were four lines of content.",
            { "->": "$r", "var": true },
            null
          ]
        }
      ],
      {
        "c-0": [
          "ev",
          { "^->": "0.c-0.$r2" },
          "/ev",
          { "temp=": "$r" },
          { "->": "0.2.s" },
          [{ "#n": "$r2" }],
          "\n",
          { "->": "Step1_Loop" },
          { "->": "0.g-0" },
          { "#f": 5 }
        ],
        "c-1": [
          "ev",
          { "^->": "0.c-1.$r2" },
          "/ev",
          { "temp=": "$r" },
          { "->": "0.3.s" },
          [{ "#n": "$r2" }],
          "\n",
          { "->": "AnotherOption" },
          { "->": "0.g-0" },
          { "#f": 5 }
        ],
        "c-2": [
          "ev",
          { "^->": "0.c-2.$r2" },
          "/ev",
          { "temp=": "$r" },
          { "->": "0.4.s" },
          [{ "#n": "$r2" }],
          "\n",
          { "->": "0.g-0" },
          { "#f": 5 }
        ],
        "g-0": [
          "^They lived happily ever after.",
          "\n",
          "end",
          ["done", { "#f": 5, "#n": "g-1" }],
          { "#f": 5 }
        ]
      }
    ],
    "done",
    {
      "Step1_Loop": [
        "^The first step of the loop is here.",
        "\n",
        "ev",
        { "VAR?": "thisisavariable" },
        1,
        "+",
        "/ev",
        { "VAR=": "thisisavariable", "re": true },
        { "->": "Step2_Loop" },
        { "#f": 1 }
      ],
      "Step2_Loop": [
        "^The second step of the loop is here.",
        "\n",
        { "->": "Step1_Loop" },
        { "#f": 1 }
      ],
      "AnotherOption": [
        "^This is another option.",
        "\n",
        "ev",
        { "VAR?": "thisisavariable" },
        1,
        "+",
        "/ev",
        { "VAR=": "thisisavariable", "re": true },
        "ev",
        { "VAR?": "thisisavariable" },
        "out",
        "/ev",
        "\n",
        "end",
        { "#f": 1 }
      ],
      "global decl": [
        "ev",
        0,
        { "VAR=": "thisisavariable" },
        "/ev",
        "end",
        null
      ],
      "#f": 1
    }
  ],
  "listDefs": {}
}
